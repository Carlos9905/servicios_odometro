<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_servicios_odometros_tree" model="ir.ui.view">
        <field name="name">servicios.odometros.tree</field>
        <field name="model">servicios.odometros</field>
        <field name="arch" type="xml">
            <tree string="Servicios y Odometros" editable="bottom">
                <field name="name"/>
                <field name="unidad"/>
                <field name="operador" />
                <field name="costo" />
                <field name="fecha" />
                <field name="tipo_carga"/>
                <field name="odo_inicial" />
                <field name="odo_final" />
                <field name="litros" />
                <field name="km_acumulado" />
                <field name="ren_optimo" />
            </tree>
        </field>
    </record>
</odoo>