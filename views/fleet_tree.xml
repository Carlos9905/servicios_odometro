        <field name="model">servicios.odometros</field>
        <field name="arch" type="xml">
            <tree string="Servicios y Odometros" editable="bottom">
                <field name="name"/>
                <field name="unidad"/>
                <field name="costo" />
                <field name="fecha" />
                <field name="tipo_carga" />
